{
  "start": {
    "prefix": ["start"],
    "body": [
      "#!/usr/bin/env python",
      "# -*- encoding: utf-8 -*-",
      "# @file ${TM_FILENAME}",
      "# @author: wujiangu",
      "# @date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}",
      "# @description: ${1}"
    ]
  },
  "main": {
    "prefix": ["main"],
    "body": ["if __name__ == '__main__':", "    ${1}"]
  },
  "log": {
    "prefix": ["log"],
    "body": [
      "# WARNING: log remember to delete",
      "print('=' * 50)",
      "print(' ' * 8 + f'${1}')",
      "print('=' * 50)"
    ]
  }
}
